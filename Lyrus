                        c1(b6, eg["Colour"])
                        b6.CharacterAdded:Connect(
                            function(ck)
                                local ef = ck:WaitForChild "Head"
                                if ef then
                                    c1(b6, eg["Colour"])
                                    cv(ef, eg["Name"], eg["Colour"])
                                end
                            end
                        )
                    end
                end
            end
        )
        local ei
        ei =
            b6.Chatted:Connect(
            function(bB)
                local cA = cI(b6, bB)
                if cA then
                    ei:Disconnect()
                end
            end
        )
    end
