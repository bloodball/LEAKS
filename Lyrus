AddCommand(
    function()
        D = not D
        if D then
            local eo = workspace:GetDescendants()
            for bA = 1, #eo do
                local bO = eo[bA]
                if string.find(bO.ClassName:lower(), "seat") then
                    bO.Parent = CoreGui
                end
            end
        else
            local ep = CoreGui:GetDescendants()
            for bA = 1, #ep do
                local bO = ep[bA]
