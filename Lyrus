coroutine.resume(
    coroutine.create(
        function()
            local ey, ez, eA =
                syn_io_listdir or list_files,
                syn_io_isfolder or isfolder,
                syn_io_makefolder or makefolder
            if ey and ez and eA then
                if not ez "LyrusAdminPlugins" then
                    eA("LyrusAdminPlugins")
                end
                for c0, cs in pairs(ey "LyrusAdminPlugins") do
                    local eB = loadfile(cs)
                    if not eB then
                        ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(
                            "There was a syntax error (sadly can't output it as loadfile is gay)",
                            "All"
                        )
                    else
                        local bV, bF = pcall(eB)
                        if not bV then
                            ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(
                                "Runtime Error" .. bF,
                                "All"
                            )
                        end
                    end
                end
            end
            aD.Text = "Commands | Total Commands: " .. #aY
            for bA = 1, #aY do
                local bQ = aY[bA]
                local cw, bz, eC, eD = bQ.Name, bQ.Args, bQ.Alias, bQ.Help
                de(UDim2.new(0.0008, 0, 0.002, -35 + bA * 29), cw, eD)
                coroutine.resume(
                    coroutine.create(
                        function()
                            dc(cw, bz)
                        end
                    )
                )
                coroutine.resume(
                    coroutine.create(
                        function()
                            for bA = 1, #eC do
                                dc("[Alias] " .. eC[bA], bz)
                            end
                        end
                    )
                )
            end
            while wait() do
                local bt = GetChar()
                coroutine.resume(
                    coroutine.create(
                        function()
                            if bt and bt:FindFirstChildOfClass "Humanoid" then
                                if workspace.Gravity < a8 then
                                    if game.PlaceId == 455366377 and not bt:FindFirstChild "RealHumanoidRootPart" then
                                        bt.Humanoid:ChangeState(3)
                                        bt.Humanoid.PlatformStand = false
                                        wait(0.2)
                                    end
                                    bt.Humanoid:ChangeState(8)
                                end
                                if game.PlaceId == 455366377 then
                                    if bt.Humanoid.HipHeight > 0 or j and not v then
                                        bt.Humanoid:ChangeState(3)
                                        bt.Humanoid.PlatformStand = false
                                        RunService.RenderStepped:Wait()
                                        bt.Humanoid:ChangeState(8)
                                        wait(2)
                                    end
                                end
                            end
                            local eE = tick()
                            ReplicatedStorage.DefaultChatSystemChatEvents.MutePlayerRequest:InvokeServer()
                            an = tick() - eE
                        end
                    )
                )
                local b_ = bt:FindFirstChildOfClass "Tool"
                if
                    b and ab and b_ and b_:FindFirstChild("Clips", true) and ab:FindFirstChildOfClass "Humanoid" and
                        ab.Humanoid.Health > 0 and
                        not ab:FindFirstChildOfClass "ForceField"
                 then
                    if not dy(ab) and cn(Players:GetPlayerFromCharacter(ab), "Bone") ~= "Yes" then
                        if
                            ab:FindFirstChild "Head" and bt:FindFirstChild "Head" and
                                (ab.Head.Position - bt.Head.Position).magnitude <= be[b_.Name]
                         then
                            if game.PlaceId == 455366377 then
                                LP.Backpack.Input:FireServer(
                                    "ml",
                                    {
                                        ["mousehit"] = dD(),
                                        ["shift"] = UserInput:IsKeyDown(Enum.KeyCode.LeftShift),
                                        ["velo"] = bt.Head.Velocity.magnitude
                                    }
                                )
                            else
                                b_.Fire:FireServer(dD())
                            end
                        end
                    end
                end
