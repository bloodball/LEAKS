            if bC[1] == "image" then
                if bC[2] then
                    aH.Image = "http://www.roblox.com/asset/?id=" .. bC[2]
                end
            elseif bC[1] == "none" then
                aH.Image = ""
            elseif bC[1] == "default" then
                aH.Image = "http://www.roblox.com/asset/?id=2683269674"
            end
            updateHotkeys(S)
        end
    end,
    "commandbarimage",
