getgenv().initalizeHotkeys = function(bh)
    writefile(bh, HttpService:JSONEncode(bb))
    local bi = HttpService:JSONDecode(readfile(bh))
    b0 = bi.Keys
    ag = bi.ClicktpKey
    a4 = bi.SprintSpeed
    O = bi.AimMode
    P = bi.AimlockMode
    Y = bi.AimbotVelocity
    CmdBarImage = bi.CmdBarImage
    CmdBarKey = bi.CmdBarKey
end
getgenv().updateHotkeys = function(bj)
    if not readfile or not writefile then
        return
    end
    local bk = {
        Keys = b0,
        ClickTpKey = ag,
        SprintSpeed = a4,
        CrouchSpeed = a0,
        AimMode = O,
        AimlockMode = P,
        AimbotVelocity = Y,
        CmdBarImage = CmdBarImage,
        CmdBarKey = CmdBarKey
    }
    writefile(bj, HttpService:JSONEncode(bk))
end
coroutine.resume(
    coroutine.create(
        function()
            getgenv().runHotkeys = function(bl)
                local bm = HttpService:JSONDecode(readfile(bl))
                b0 = bm.Keys
                ag = bm.ClickTpKey or ""
                a4 = bm.SprintSpeed or 25
                a0 = bm.CrouchSpeed or 16
                O = bm.AimMode or "Prediction"
                P = bm.AimlockMode or "LeftClick"
                Y = bm.AimbotVelocity or 5
                CmdBarImage = bm.CmdBarImage or "http://www.roblox.com/asset/?id=2683269674"
                CmdBarKey = bm.CmdBarKey or "Minus"
            end
            if readfile and writefile then
                local bn = pcall(readfile, S)
                if not bn then
                    initalizeHotkeys(S)
                else
                    runHotkeys(S)
                end
            end
        end
    )
)
getgenv().notif = function(bo, bp, bq, br)
    StarterGui:SetCore("SendNotification", {["Title"] = bo, ["Text"] = bp, ["Duration"] = bq, ["Icon"] = br})
end
getgenv().Teleport = function(bs)
    if typeof(bs) == "Instance" then
        bs = bs.CFrame
    end
    if typeof(bs) == "Vector3" then
        bs = CFrame.new(bs)
    end
    if typeof(bs) == "CFrame" then
        local bt = GetChar()
        local bu = bt:FindFirstChild "RealHumanoidRootPart" or bt:FindFirstChild "Torso"
        if bu and not bt:FindFirstChild "RealHumanoidRootPart" or (bs.p - bu.CFrame.p).magnitude < 50 then
            bu.CFrame = bs
        else
            TweenService:Create(
                bu,
                TweenInfo.new(3.2, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut),
                {CFrame = bs}
            ):Play()
        end
    end
end
getgenv().AddCommand = function(bv, bw, bx, by, bz)
    aY[#aY + 1] = {["Function"] = bv, ["Name"] = bw, ["Alias"] = bx, ["Help"] = by, ["Args"] = bz}
end
getgenv().FindCommand = function(bw)
    for bA = 1, #aY do
        if aY[bA].Name == bw or f and table.find(aY[bA].Alias, bw) then
            return aY[bA].Function
        end
    end
end
getgenv().CheckCommand = function(bB)
    local bC = string.split(bB:lower(), " ")
    local bw = table.remove(bC, 1)
    local bD = FindCommand(bw)
    if bD then
        local bE, bF = pcall(bD, bC)
        if not bE then
            notif("Command Errored: " .. bw, "Send This To Liamriley101#6485: " .. bF, 10, nil)
        end
    end
end
getgenv().PlrFinder = function(bG)
    if bG then
        local bG = bG:lower()
        local bH = Players:GetPlayers()
        if #bG == 2 and bG == "me" then
            return LP
        end
        if #bG == 3 and bG == "all" or #bG == 5 and bG == "users" then
            return bH
        end
        for bA = 1, #bH do
            if bH[bA].Name:lower():sub(1, #bG) == bG then
                return bH[bA]
            end
        end
    end
end
getgenv().find = function(bI)
    local bJ = workspace:GetChildren()
    for bA = 1, #bJ do
        local bK = bJ[bA]
        local bL = bK:FindFirstChild "Model"
        if bK.Name == "RandomSpawner" and bL then
            local bM = bL.Handle
            if bM:FindFirstChildOfClass "MeshPart" then
                if bd[bI] and string.find(bM:FindFirstChildOfClass "MeshPart".MeshId, bd[bI]) then
                    return bK, "CashItemEsp"
                end
            end
            if bM:FindFirstChild "Fire" then
                if bd[bI] and string.find(bM.Fire.SoundId, bd[bI]) then
                    return bK, "GunItemEsp"
                end
            end
        end
    end
    return "None"
end
getgenv().farm = function(bI)
    if bI == "all" then
        local bN = workspace:GetChildren()
        for bA = 1, #bN do
            local bO = bN[bA]
            if bO.Name == "RandomSpawner" then
                Teleport(bO.CFrame)
                bO.DescendantRemoving:Wait()
            end
        end
    end
    local bK = find(bI)
    if bK == "None" then
        notif("There is none of " .. bI, "to farm", 5, nil)
        return
    end
    Teleport(bK.CFrame)
end
local function bP(b6, bB)
    if bB:sub(1, 1) == "=" then
        local bC = string.split(bB:sub(2), " ")
        local bQ = b5[table.remove(bC, 1)]
        local bR = PlrFinder(table.remove(bC, 1))
        if bQ and bR then
            bQ["Func"](bR, table.concat(bC, " "), b6)
        end
    end
end
local function bS(bT)
    local bU
    local bV, bF =
        pcall(
        function()
            bU = Enum.KeyCode[bT]
        end
    )
    if not bV then
        bU = Enum.KeyCode[bT:upper()]
    end
    return bU
end
local function bW(bX, bY)
    for bZ, b_ in pairs(bX:GetChildren()) do
        if b_:IsA "Tool" and b_:FindFirstChild "Fire" then
            for c0, bs in pairs(b_:GetDescendants()) do
                if bs:IsA "UnionOperation" or bs:IsA "Part" or bs:IsA "MeshPart" then
                    if bs:IsA "UnionOperation" then
                        bs.UsePartColor = true
                    end
                    bs.Material = "ForceField"
                    bs.Color = bY
                end
            end
        end
    end
end
local function c1(c2, bY)
    if c2 and c2.Character and c2.UserId ~= 567153118 then
        bW(c2.Backpack, bY)
        bW(c2.Character, bY)
        c2.Character.ChildAdded:Connect(
            function()
                bW(c2.Character, bY)
            end
        )
    end
end
local function c3(c4)
    local c5 = Instance.new("BodyPosition", c4)
    c5.P = c5.P * 8
    c5.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
    if A then
        aj = Instance.new("BodyAngularVelocity", c4)
        aj.AngularVelocity = Vector3.new(0, 9e9, 0)
        aj.MaxTorque = Vector3.new(0, 9e9, 0)
    end
    return c5
end
local LP = game.Players.LocalPlayer
local c6 = LP:GetMouse()
function sFLY()
    repeat
        wait()
    until LP and LP.Character and LP.Character:FindFirstChild("Torso")
    game.Players.LocalPlayer.Character.Torso.Anchored = true
    repeat
        wait()
    until c6
    local c7 = game.Players.LocalPlayer.Character.Torso
    local c8 = {F = 0, B = 0, L = 0, R = 0}
    local c9 = {F = 0, B = 0, L = 0, R = 0}
    local ca = 0
    local function cb()
        v = true
        local cc = Instance.new("BodyGyro", c7)
        local cd = Instance.new("BodyVelocity", c7)
        cc.P = 9e4
        cc.maxTorque = Vector3.new(9e9, 9e9, 9e9)
        cc.cframe = c7.CFrame
        cd.velocity = Vector3.new(0, 0.1, 0)
        cd.maxForce = Vector3.new(9e9, 9e9, 9e9)
        spawn(
            function()
                if not j then
                    CheckCommand("airwalk")
                end
                repeat
                    wait()
                    if c8.L + c8.R ~= 0 or c8.F + c8.B ~= 0 then
                        ca = 50
                    elseif not (c8.L + c8.R ~= 0 or c8.F + c8.B ~= 0) and ca ~= 0 then
                        ca = 0
                    end
                    if c8.L + c8.R ~= 0 or c8.F + c8.B ~= 0 then
                        cd.velocity =
                            (workspace.CurrentCamera.CoordinateFrame.lookVector * (c8.F + c8.B) +
                            workspace.CurrentCamera.CoordinateFrame * CFrame.new(c8.L + c8.R, (c8.F + c8.B) * 0.2, 0).p -
                            workspace.CurrentCamera.CoordinateFrame.p) *
                            ca
                        c9 = {F = c8.F, B = c8.B, L = c8.L, R = c8.R}
                    elseif c8.L + c8.R == 0 and c8.F + c8.B == 0 and ca ~= 0 then
                        cd.velocity =
                            (workspace.CurrentCamera.CoordinateFrame.lookVector * (c9.F + c9.B) +
                            workspace.CurrentCamera.CoordinateFrame * CFrame.new(c9.L + c9.R, (c9.F + c9.B) * 0.2, 0).p -
                            workspace.CurrentCamera.CoordinateFrame.p) *
                            ca
                    else
                        cd.velocity = Vector3.new(0, 0.1, 0)
                    end
                    cc.cframe = workspace.CurrentCamera.CoordinateFrame
                until not v
                if j then
                    CheckCommand("airwalk")
                end
                c8 = {F = 0, B = 0, L = 0, R = 0}
                c9 = {F = 0, B = 0, L = 0, R = 0}
                ca = 0
                cc:destroy()
                cd:destroy()
            end
        )
    end
    c6.KeyDown:connect(
        function(ce)
            if ce:lower() == "w" then
                c8.F = a1
            elseif ce:lower() == "s" then
                c8.B = -a1
            elseif ce:lower() == "a" then
                c8.L = -a1
            elseif ce:lower() == "d" then
                c8.R = a1
            end
        end
    )
    c6.KeyUp:connect(
        function(ce)
            if ce:lower() == "w" then
                c8.F = 0
            elseif ce:lower() == "s" then
                c8.B = 0
            elseif ce:lower() == "a" then
                c8.L = 0
            elseif ce:lower() == "d" then
                c8.R = 0
            end
        end
    )
    cb()
    game.Players.LocalPlayer.Character.Torso.Anchored = false
end
function Fly()
    if game.Players.LocalPlayer.PlayerGui:WaitForChild("LocalScript", math.huge) then
        game.Players.LocalPlayer.PlayerGui:WaitForChild("LocalScript", math.huge).Disabled = true
    end
    wait()
    sFLY()
    wait(0.1)
    if game.Players.LocalPlayer.PlayerGui:WaitForChild("LocalScript", math.huge) then
        game.Players.LocalPlayer.PlayerGui:WaitForChild("LocalScript", math.huge).Disabled = false
    end
end
local function cf(b6)
    local cg = Instance.new("BoxHandleAdornment", CoreGui.RobloxGui)
    cg.Adornee = b6.Character.Head
    cg.Size = Vector3.new(7, 10, 7)
    cg.SizeRelativeOffset = Vector3.new(0, -1, 0)
    cg.Transparency = 1
    local ch, ci
    ch =
        cg.MouseButton1Down:Connect(
        function()
            if e and P == "Closest" then
                ab = b6.Character
                local cj
                cj =
                    Players:GetPlayerFromCharacter(ab).CharacterAdded:Connect(
                    function(ck)
                        if tostring(ck) == tostring(ab) then
                            ab = ck
                        else
                            cj:Disconnect()
                            cj = nil
                        end
                    end
                )
            else
                cg:Destroy()
            end
        end
    )
    ci =
        b6.CharacterRemoving:Connect(
        function()
            cg:Destroy()
            ch:Disconnect()
            ch = nil
            ci:Disconnect()
            ci = nil
        end
    )
end
local function cl(cm)
    return cm:FindFirstChildOfClass "Humanoid" and math.floor(cm.Humanoid.Health) or "No Humanoid"
end
local function cn(b6, bK)
    if b6 then
        local co = b6.Character:FindFirstChild(bK, true) or b6.Backpack:FindFirstChild(bK, true)
        return co and "Yes" or "No"
    end
end
local function cp(bs)
    local cq = bs:FindFirstChildOfClass "BillboardGui"
    if bs:IsA "BasePart" and cq then
        bs = bs.Parent
        cq:Destroy()
        return
    end
    for bA = 1, #aZ do
        local cr = aZ[bA]
        if cr then
            local b6 = cr["Player"]
            if b6 == bs then
                for bA, cs in pairs(cr) do
                    if cs ~= b6 then
                        if bA ~= "Box" then
                            cs:Remove()
                        else
                            table.foreach(
                                cs,
                                function(ct, cu)
                                    cu:Remove()
                                end
                            )
                        end
                    end
                end
                table.remove(aZ, bA)
            end
        end
    end
end
local function cv(bs, cw, bY, cx)
    local b6 = PlrFinder(bs.Parent.Name)
    if b6 and L and not bY then
        cp(b6)
        aZ[#aZ + 1] = {
            ["Player"] = b6,
            ["Text"] = Drawing.new "Text",
            ["Box"] = {Drawing.new "Line", Drawing.new "Line", Drawing.new "Line"}
        }
    else
        local cq = bs:FindFirstChildOfClass "BillboardGui"
        if cq then
            cq:Destroy()
        end
        local cy = Instance.new("BillboardGui", bs)
        local cz = Instance.new("TextLabel", cy)
        cy.Adornee = bs
        cy.Size = UDim2.new(0, 100, 0, 100)
        cy.StudsOffset = Vector3.new(0, 1.3, 0)
        cy.AlwaysOnTop = true
        cz.BackgroundTransparency = 1
        cz.Size = UDim2.new(1, 0, 0, 40)
        cz.TextColor3 = bY or aa
        cz.TextStrokeTransparency = 0.5
        cz.TextSize = 8
        local b6
        if cw ~= "GunItemEsp" and cw ~= "CashItemEsp" then
            b6 = PlrFinder(cw)
        else
            cw = cw:gsub("ItemEsp", "")
        end
        if b6 then
            if not cx then
                local cA = aX[b6] and "Yes" or "No"
                cz.Text =
                    cw ..
                    " | LyAdmin User: " ..
                        cA ..
                            "\nHas (Gamepasses) Glock: " ..
                                cn(b6, "Glock") ..
                                    " | Shotty: " .. cn(b6, "Shotty") .. " | Vest: " .. cn(b6, "BulletResist")
            else
                local cB = ba[b6.UserId]["Reason"]
                cz.Text = "[Blacklisted] " .. b6.Name .. " Reason: " .. cB
            end
        else
            cz.Text = cw
        end
    end
end
local function cC(cD)
    for bA, cs in pairs(workspace:GetDescendants()) do
        if
            cs:IsA "Part" and not cs.Parent:FindFirstChild "Head" and not cs.Parent.Parent:FindFirstChild "Head" and
                cs.Size.Y ~= 1
         then
            local cE = cs:FindFirstChildOfClass "SelectionBox"
            local cF = cs:FindFirstChildOfClass "IntValue"
            if cF then
                cs.Transparency = cF.Value
                cF:Destroy()
                if cE then
                    cE:Destroy()
                end
            else
                if cD == "wireframe" then
                    local cG = Instance.new("SelectionBox", cs)
                    cG.Adornee = cs
                    cG.LineThickness = 0.001
                    cG.SurfaceTransparency = 1
                    cG.Color3 = Color3.fromRGB(124, 0, 0)
                    b4[#b4 + 1] = {cG, cs}
                end
                local cH = Instance.new("IntValue", cs)
                cH.Value = cs.Transparency
                cs.Transparency = 1
            end
        end
    end
end
local function cI(b6, bB)
    if bB == "Lyrus Admin" then
        aX[b6] = true
        return true
    end
end
local function cJ(cr, cK, b6)
    table.foreach(
        cr,
        function(ct, cu)
            if ct == "Box" then
                table.foreach(
                    cu,
                    function(cL, cM)
                        cM.Visible = cK
                        local cN = tostring(b6) == tostring(af) or tostring(b6) == tostring(ab)
                        cM.Color = cN and Color3.fromRGB(255, 0, 0) or aa
                    end
                )
            else
                if typeof(cu) ~= "Instance" then
                    cu.Visible = cK
                    cu.Color = aa
                end
            end
        end
    )
end
local function cO(cP)
    return workspace.CurrentCamera:WorldToViewportPoint(cP)
end
local function cQ(cR)
    local cS = GetChar().Humanoid:GetPlayingAnimationTracks()
    for bA = 1, #cS do
        local cT = cS[bA]
        if cT.Animation.AnimationId == "rbxassetid://" .. cR then
            cT:Stop()
        end
    end
end
local function cU(cV, cW)
    if game.PlaceId ~= 455366377 then
        return
    end
    local bu = GetChar():FindFirstChild "RealHumanoidRootPart" or GetChar():FindFirstChild "Torso"
    local cT = GetChar().Humanoid:LoadAnimation(ax)
    bu.CFrame = bu.CFrame * CFrame.new(math.random(20, 45), 0, math.random(1, 5))
    wait()
    q = true
    repeat
        cT:play(0.1, 1, 10)
        bu.CFrame = b1[cV]:FindFirstChildOfClass "Part".CFrame + Vector3.new(0, 1.3, 0)
        RunService.Heartbeat:wait()
    until b1[cV]:FindFirstChildOfClass "Part".BrickColor == BrickColor.new "Bright red" or
        GetChar():FindFirstChild("Bone", true) or
        GetChar().Humanoid.Health == 0
    bu.CFrame = cW
    q = false
    return true
end
local function cX(cY)
    if cY <= a5 and B and not I then
        if cU("burger", GetChar().Head.CFrame) then
            local cZ = LP.Backpack:FindFirstChild "Burger"
            if cZ then
                cZ.Parent = GetChar()
                cZ:Activate()
                repeat
                    RunService.Heartbeat:Wait()
                until cZ.Parent ~= GetChar()
            end
        end
        if cU("drink", GetChar().Head.CFrame) then
            local c_ = LP.Backpack:FindFirstChild "Drink"
            if c_ then
                c_.Parent = GetChar()
                c_:Activate()
            end
        end
    end
end
local function d0(d1, cV)
    local d2 = false
    local d3, d4, d5
    local function d6(d7)
        local d8 = d7.Position - d4
        TweenService:Create(
            d1,
            TweenInfo.new(0.055, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut),
            {Position = UDim2.new(d5.X.Scale, d5.X.Offset + d8.X, d5.Y.Scale, d5.Y.Offset + d8.Y)}
        ):Play()
    end
    d1.InputBegan:Connect(
        function(d7)
            if d7.UserInputType == Enum.UserInputType.MouseButton1 then
                d2 = true
                d4 = d7.Position
                d5 = d1.Position
                d7.Changed:Connect(
                    function()
                        if d7.UserInputState == Enum.UserInputState.End then
                            d2 = false
                        end
                    end
                )
            end
        end
    )
    d1.InputChanged:Connect(
        function(d7)
            if d7.UserInputType == Enum.UserInputType.MouseMovement then
                d3 = d7
            end
        end
    )
    UserInput.InputChanged:Connect(
        function(d7)
            if d7 == d3 and d2 then
                d6(d7)
            end
        end
    )
end
local function d9(cP, da, cu)
    local db = Instance.new("TextButton", aL)
    db.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    db.BackgroundTransparency = 1
    db.Position = cP
    db.Size = UDim2.new(0, 480, 0, 50)
    db.Font = Enum.Font.SourceSans
    db.TextColor3 = Color3.fromRGB(170, 0, 0)
    db.TextSize = 25
    db.Text = da
    db.TextWrapped = true
    db.MouseButton1Click:Connect(
        function()
            if da == "All" then
                RainbowHats = "All"
                aJ.Visible = false
            else
                LP.Backpack.Stank:FireServer("rep", cu.Parent)
                aJ.Visible = false
                RainbowHats = true
            end
        end
    )
    d0(aJ, db)
end
