        if dk == Enum.HumanoidStateType.FallingDown or dk == Enum.HumanoidStateType.PlatformStanding then
            LP.Character.Humanoid.PlatformStand = false
            LP.Character.Humanoid:ChangeState(8)
        end
    end
end
local function dl(bt)
    if bt then
        local b_ = bt:FindFirstChildOfClass "Tool"
        if b_ then
            return b_, b_:FindFirstChild "Fire" and "shot you" or "hit you"
        end
    end
end
