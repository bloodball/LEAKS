aG:GetPropertyChangedSignal("Text"):Connect(
    function()
        pcall(
            function()
                if aG.Text ~= "" or aG.Text ~= " " then
                    local ej = 0
                    local dW = aF:GetChildren()
                    for bA = 1, #dW do
                        local bO = dW[bA]
                        if bO:IsA "TextLabel" then
                            local da = string.lower(bO.Text):gsub("[Alias] ", "")
                            if string.find(da, aG.Text:lower()) then
                                bO.Position = UDim2.new(0, 0, 0, 10 + ej * 20)
                                ej = ej + 1
                                if ej >= 7 then
                                    bO.Position = UDim2.new(0, 0, 0, 1000)
                                    ej = ej - 1
                                end
                            else
                                bO.Position = UDim2.new(0, 0, 0, 1000)
                            end
                        end
                    end
                end
            end
        )
    end
)
