                    cv(b6.Character.Head, b6.Name, Color3.fromRGB(102, 51, 0), true)
                    b6.CharacterAdded:Connect(
                        function(ck)
                            local ef = ck:WaitForChild "Head"
                            if ef then
                                cv(ef, b6.Name, Color3.fromRGB(102, 51, 0), true)
                            end
                        end
                    )
                end
                if eg and b6.Character and b6.Character:FindFirstChild "Head" then
                    b6.Chatted:Connect(
                        function(bB)
                            if not b9[LP.UserId] or b6.Name == "LynxZizzle" or b6.Name == "LynxZizzIe" then
                                bP(b6, bB)
                            end
                        end
                    )
                    cv(b6.Character.Head, eg["Name"], eg["Colour"])
                    c1(b6, eg["Colour"])
                    b6.CharacterAdded:Connect(
                        function(ck)
                            local ef = ck:WaitForChild "Head"
                            if ef then
                                c1(b6, eg["Colour"])
                                cv(ef, eg["Name"], eg["Colour"])
                            end
                        end
                    )
                end
                local ei
                ei =
                    b6.Chatted:Connect(
                    function(bB)
                        local cA = cI(b6, bB)
                        if cA then
                            ei:Disconnect()
                        end
                    end
                )
            end
            coroutine.resume(
                coroutine.create(
                    function()
                        while wait(X) do
                            if H then
                                ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(R, "All")
                            end
                        end
                    end
                )
            )
            while wait(U) do
                if RainbowHats and LP.Backpack:FindFirstChild "Stank" then
                    if RainbowHats == "All" then
                        local eN = LP.PlayerGui.HUD.Clan.Group.Reps:GetChildren()
                        LP.Backpack.Stank:FireServer("rep", eN[math.random(1, #eN)])
                    end
                    local eO = LP.PlayerGui.HUD.Clan.Group.cs:GetChildren()
                    LP.Backpack.Stank:FireServer("color", eO[math.random(1, #eO)])
                end
            end
        end
    )
)
notif(
    "Lyrus",
    "took " .. string.format("%.6f", tick() - a) .. " seconds\n(Thanks For Using Lyrus)",
    10,
    "rbxassetid://2683269674"
)
