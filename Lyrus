                            local eI = Vector3.new(2, 3, 0) * ef.Size.Y / 2 * 2
                            local eJ = cO((eG.CFrame * CFrame.new(eI.X, eI.Y, 0)).p)
                            local eK = cO((eG.CFrame * CFrame.new(-eI.X, eI.Y, 0)).p)
                            local eL = cO((eG.CFrame * CFrame.new(eI.X, -eI.Y, 0)).p)
                            local eM = cO((eG.CFrame * CFrame.new(-eI.X, -eI.Y, 0)).p)
