UserInput.InputBegan:Connect(
    function(e9)
        local bt = GetChar()
        local bu = bt:FindFirstChild "RealHumanoidRootPart" or bt:FindFirstChild "Torso"
        local dz = c6.Target
        if UserInput:GetFocusedTextBox() then
            return
        end
        if
            not bt:FindFirstChildOfClass "Tool" and P == "LeftClick" and
                e9.UserInputType == Enum.UserInputType.MouseButton1 or
                P == "RightClick" and e9.UserInputType == Enum.UserInputType.MouseButton2
         then
            if dz and dz.Parent then
                local ea = dz.Parent
                if not Players:GetPlayerFromCharacter(ea) then
                    ea = ea.Parent
                end
                if not Players:GetPlayerFromCharacter(ea) then
                    return
                end
                if ea ~= bt and ea ~= ab and e then
                    ab = ea
                    local cj
                    cj =
                        Players:GetPlayerFromCharacter(ea).CharacterAdded:Connect(
                        function(ck)
                            if tostring(ck) == tostring(ab) then
                                ab = ck
                            else
                                cj:Disconnect()
                                cj = nil
                            end
                        end
                    )
                    notif("AimlockTarget", "Has been set to " .. ab.Name, 5, nil)
                end
            end
        end
        if ae then
            local bT = e9.KeyCode.Name
            if bT ~= "Unknown" and bT ~= "Return" and Keycode ~= "Slash" then
                if ae == "CmdBar" then
                    CmdBarKey = bT
                    notif("CommandBarKey", "Has been set to the hotkey: " .. bT, 5, nil)
                    ae = nil
                    aQ.Visible = false
                elseif ae == "AnyCmd" and aV.Text ~= "" then
                    for ct, e9 in pairs(b0) do
                        if e9:match("[%a%d]+$") == bT then
                            table.remove(b0, ct)
                        end
                    end
                    b0[#b0 + 1] = aV.Text .. "||" .. bT
                    notif(aV.text, "Has been set to the hotkey: " .. bT, 5, nil)
                    ae = nil
                    aV.Text = ""
                    aQ.Visible = false
                end
                updateHotkeys(S)
            end
        end
        if dz and e9.UserInputType == Enum.UserInputType.MouseButton2 then
            local dz = dz.Parent
            if dz and dz:FindFirstChild "Click" and dz:FindFirstChild "Locker" then
                if dz.Locker.Value then
                    dz.Lock.ClickDetector:FindFirstChildOfClass "RemoteEvent":FireServer()
                    dz.Click.ClickDetector:FindFirstChildOfClass "RemoteEvent":FireServer()
                else
                    dz.Click.ClickDetector:FindFirstChildOfClass "RemoteEvent":FireServer()
                    dz.Lock.ClickDetector:FindFirstChildOfClass "RemoteEvent":FireServer()
                end
            end
        end
        if ag and ag ~= "" and e9.KeyCode == Enum.KeyCode[ag:upper()] and dz then
            Teleport(CFrame.new(c6.Hit.p + Vector3.new(0, 5, 0)))
        end
        for bA, cs in pairs(b0) do
            local bT = bS(cs:match "[%a%d]+$")
            if bT == e9.KeyCode then
                CheckCommand(cs:match "^[%w%s]+")
            end
        end
        if e9.KeyCode == Enum.KeyCode.LeftControl then
            bf["Control"] = true
            if j then
                aB.Size = Vector3.new(0, 0, 0)
            end
            if E and a0 == 8 then
                return
            end
            bt.Humanoid.WalkSpeed = a0
        end
        if e9.KeyCode == Enum.KeyCode.LeftShift then
            bf["Shift"] = true
            if E and a4 == 25 then
                return
            end
            bt.Humanoid.WalkSpeed = a4
        end
        if e9.KeyCode == Enum.KeyCode.W then
            bf["W"] = true
        end
        if e9.KeyCode == Enum.KeyCode.A then
            bf["A"] = true
        end
        if e9.KeyCode == Enum.KeyCode.S then
            bf["S"] = true
        end
        if e9.KeyCode == Enum.KeyCode.D then
            bf["D"] = true
        end
        if e9.KeyCode == Enum.KeyCode.Space then
            if j then
                bu.CFrame = bu.CFrame + Vector3.new(0, 5, 0)
            end
        end
        if
            e9.KeyCode == Enum.KeyCode.E and bt:FindFirstChildOfClass "Tool" and
                bt:FindFirstChildOfClass "Tool":FindFirstChild "Clips" and
                not bt:FindFirstChild("Bone", true) and
                z
         then
            if game.PlaceId == 455366377 then
                local eb = bt:FindFirstChildOfClass "Tool"
                eb.Parent = LP.Backpack
                wait()
                local ec = LP.Backpack.Punch
                ec.Parent = bt
                LP.Backpack.Input:FireServer("e", {})
                wait(1)
                ec.Parent = LP.Backpack
                wait()
                eb.Parent = bt
            else
                LP.Backpack.ServerTraits.Finish:FireServer(LP.Backpack.Punch)
            end
        end
        if getrawmetatable then
            if e9.KeyCode == Enum.KeyCode[CmdBarKey] then
                wait()
                aG:CaptureFocus()
                aF:TweenPosition(UDim2.new(0.5, 0, 0.5, 0), "In", "Sine", 0.5, true)
                local ed = UserInput.TextBoxFocusReleased:Wait()
                CheckCommand(ed.Text)
                ed.Text = ""
                aF:TweenPosition(UDim2.new(1.5, 0, 1.5, 0), "Out", "Quad", 0.5, true)
            end
        else
            if e9.KeyCode == Enum.KeyCode.Minus then
                wait()
                aG:CaptureFocus()
                aF:TweenPosition(UDim2.new(0.5, 0, 0.5, 0), "In", "Sine", 0.5, true)
                local ed = UserInput.TextBoxFocusReleased:Wait()
                CheckCommand(ed.Text)
                ed.Text = ""
                aF:TweenPosition(UDim2.new(1.5, 0, 1.5, 0), "Out", "Quad", 0.5, true)
            end
        end
        if bt:FindFirstChild "GravGun" then
            if e9.KeyCode == Enum.KeyCode.Q and _ > 5 then
                _ = _ - 5
            end
            if e9.KeyCode == Enum.KeyCode.V then
                _ = _ + 5
            end
            if e9.KeyCode == Enum.KeyCode.B then
                A = not A
                notif(
                    "WOW!",
                    "You found Grav gun seizure mode!, it has been set to " .. tostring(A),
                    5,
                    "rbxassetid://5929642434"
                )
            end
        end
    end
)
UserInput.InputEnded:Connect(
    function(e9)
        local bt = GetChar()
        if UserInput:GetFocusedTextBox() then
            return
        end
        if e9.KeyCode == Enum.KeyCode.W then
            bf["W"] = false
        end
        if e9.KeyCode == Enum.KeyCode.A then
            bf["A"] = false
        end
        if e9.KeyCode == Enum.KeyCode.S then
            bf["S"] = false
        end
        if e9.KeyCode == Enum.KeyCode.D then
            bf["D"] = false
        end
        if e9.KeyCode == Enum.KeyCode.LeftShift and a4 then
            bf["Shift"] = false
            if E and a4 == 25 then
                return
            end
            bt.Humanoid.WalkSpeed = a2
        end
        if e9.KeyCode == Enum.KeyCode.LeftControl then
            bf["Control"] = false
            if j then
                aB.Size = Vector3.new(5, 1, 5)
            end
            if E and a0 == 8 then
                return
            end
            bt.Humanoid.WalkSpeed = a2
        end
    end
)
