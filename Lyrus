    "[Image/None/Default]"
)
AddCommand(
    function(bC)
        if bC[1] then
            local b6 = PlrFinder(bC[1])
            if b6 and b6 ~= LP then
                if typeof(b6) == "table" then
                    for bA = 1, #b6 do
                        local ew = b6[bA]
                        if ew ~= LP and ew.Character and ew.Character:FindFirstChild "Head" then
                            if bC[1] == "users" and aX[ew] or bC[1] ~= "users" then
                                a_[ew] = true
                                cv(ew.Character.Head, b6.Name)
                                local ex
                                ex =
                                    ew.CharacterAdded:Connect(
                                    function(ck)
                                        local ef = ck:WaitForChild "Head"
                                        if aZ[b6] then
                                            cv(ef, b6.Name)
                                        else
                                            ex:Disconnect()
                                        end
                                    end
                                )
                            end
                        end
                    end
                else
                    if b6.Character and b6.Character:FindFirstChild "Head" then
                        a_[b6] = true
                        cv(b6.Character.Head, b6.Name)
                        local ex
                        ex =
                            b6.CharacterAdded:Connect(
                            function(ck)
                                local ef = ck:WaitForChild "Head"
                                if aZ[b6] then
                                    cv(ef, b6.Name)
                                else
                                    ex:Disconnect()
                                end
                            end
                        )
                    end
                end
            end
        end
    end,
