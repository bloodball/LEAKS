local function dt(du)
    if not GetChar():FindFirstChild "Head" then
        return
    end
    if workspace:FindFirstChild "FreecamPart" then
        workspace.FreecamPart:Destroy()
    end
    du = du or 35
    GetChar().Head.Anchored = true
    local dv = Instance.new("Part", workspace)
    dv.Name = "FreecamPart"
    dv.Position = GetChar().Head.Position + Vector3.new(0, 5, 0)
    dv.Transparency = 1
    dv.CanCollide = false
    dv.Anchored = true
    workspace.CurrentCamera.CameraSubject = dv
    while x and GetChar().Humanoid.Health > 0 and wait() do
        local cP = Vector3.new()
        local dw = (workspace.CurrentCamera.Focus.p - workspace.CurrentCamera.CoordinateFrame.p).unit
        local dx = dv.Position
        if bf["w"] then
            cP = cP + Vector3.new(0, 0, -1)
        elseif bf["a"] then
            cP = cP + Vector3.new(-1, 0, 0)
        elseif bf["s"] then
            cP = cP + Vector3.new(0, 0, 1)
        elseif bf["d"] then
            cP = cP + Vector3.new(1, 0, 0)
        elseif bf["Space"] then
            cP = cP + Vector3.new(0, 1, 0)
        elseif bf["Control"] then
            cP = cP + Vector3.new(0, -1, 0)
        end
        dv.CFrame = CFrame.new(dx, dx + dw) * CFrame.new(cP * du)
    end
    workspace.CurrentCamera.CameraSubject = GetChar()
    GetChar().Head.Anchored = false
    if workspace:FindFirstChild "FreecamPart" then
        workspace.FreecamPart:Destroy()
    end
end
local function dy(dz)
    if dz:FindFirstChild "Head" and GetChar():FindFirstChild "Head" then
        local dA = Ray.new(dz.Head.Position, GetChar().Head.Position)
        local dB = workspace:FindPartOnRay(dA)
        if dB then
            return dB:IsDescendantOf(dz)
        end
    end
end
local function dC()
    if game.PlaceId == 455366377 then
        if
            bf["Shift"] and
                (M and
                    (LP.Backpack:FindFirstChild "ServerTraits" and LP.Backpack.ServerTraits.Stann.Value > 0 or
                        GetChar():FindFirstChild "Stamina" and GetChar().Stamina.Value > 0) or
                    not M)
         then
            if E and a4 == 25 then
                return
            end
            GetChar().Humanoid.WalkSpeed = a4
            return
        end
        if bf["Control"] then
            if E and a0 == 8 then
                return
            end
            GetChar().Humanoid.WalkSpeed = a0
            return
        end
        if not M then
            GetChar().Humanoid.WalkSpeed = a2
        end
    end
end
local function dD()
    local dE
    local dF = ab.FindFirstChild(ab, "RealHumanoidRootPart") or ab.FindFirstChild(ab, "Torso")
    if dF and O == "OldPrediction" then
        dE = dF.CFrame + dF.Velocity / Y
    elseif dF and O == "Prediction" then
        dE = dF.CFrame + dF.Velocity / (an < 0.26 and 5 or 7.5) + dF.RotVelocity / (an < 0.26 and 5 or 7.5)
    elseif ab.FindFirstChild(ab, O) then
        dE = ab[O].CFrame
    end
    return dE
end
local function dG()
    local dH
    local b_ = LP.Character:FindFirstChildOfClass "Tool"
    if b_ and be[b_.Name] and (b_.Ammo.Value > 0 or b_.Clips.Value > 0) then
        return true
    else
        for bA, cs in pairs(LP.Backpack:GetChildren()) do
            if cs:IsA "Tool" and be[cs.Name] then
                if cs:FindFirstChild "Clips" then
                    if cs.Clips.Value > 0 or cs.Ammo.Value > 0 then
                        dH = cs
                        break
                    end
                end
            end
        end
        if dH then
            return dH
        else
            if
                game.PlaceId == 455366377 and GetChar():FindFirstChildOfClass "Tool" and
                    GetChar():FindFirstChildOfClass "Tool":FindFirstChild "Ammo" and
                    K and
                    not q and
                    tonumber(LP.PlayerGui.HUD.Cash.Text:sub(2)) >= 25
             then
                cU("ammo", GetChar().Head.CFrame)
                return true
            else
                GetChar():BreakJoints()
            end
            return false
        end
    end
end
coroutine.resume(
    coroutine.create(
        function()
            if getrawmetatable then
                local dI = getrawmetatable(game)
                local dJ = checkcaller or is_protosmasher_caller or Cer.isCerus
                local dK = getcallingscript or get_calling_script
                local dL = newcclosure or read_me or function(dM)
                        return dM
                    end
                local dN = getnamecallmethod or get_namecall_method
                local dO = {"WalkSpeed", "JumpPower", "HipHeight", "Health"}
                local dP = Instance.new "BodyVelocity"
                dP.Name = "Tempby"
                setreadonly(dI, false)
                local ct = dI.__index
                dI.__index =
                    dL(
                    function(self, dQ)
                        if I and dK and dK() ~= script then
                            if dQ == "HumanoidRootPart" then
                                return ct(self, "Torso")
                            end
                            if dQ == "Name" and (self:IsA "BodyVelocity" or self:IsA "BodyPosition") then
                                return dP
                            end
                        end
                        return ct(self, dQ)
                    end
                )
                local dR = dI.__newindex
                dI.__newindex =
                    dL(
                    function(self, dS, cu)
                        if dJ() then
                            return dR(self, dS, cu)
                        end
                        StarterGui:SetCore("ResetButtonCallback", true)
                        if dS == "WalkSpeed" then
                            return
                        end
                        if dS == "JumpPower" then
                            return
                        end
                        if dS == "HipHeight" then
                            return
                        end
                        if dS == "Health" then
                            return
                        end
                        if self == workspace and dS == "Gravity" then
                            return a8
                        end
                        if dS == "CFrame" and self:IsDescendantOf(LP.Character) then
                            return
                        end
                        return dR(self, dS, cu)
                    end
                )
                local dT = dI.__namecall
                dI.__namecall =
                    dL(
                    function(self, ...)
                        local dN = dN()
                        local bz = {...}
                        if dJ() then
                            if dN == "FindFirstChild" and bz[1] == "RealHumanoidRootPart" then
                                bz[1] = "HumanoidRootPart"
                                return dT(self, unpack(bz))
                            end
                            return dT(self, ...)
                        end
                        if dN == "Destroy" or dN == "Kick" then
                            if self == LP then
                                return wait(9e9)
                            end
                            if tostring(self) == "BodyGyro" or tostring(self) == "BodyVelocity" then
                                return wait(9e9)
                            end
                        end
                        if dN == "BreakJoints" and self == LP.Character then
                            return wait(9e9)
                        end
                        if dN == "FireServer" then
                            if table.find(dO, bz[1]) then
                                return wait(9e9)
                            end
                            if tostring(self) == "Fire" and e and ab then
                                return dT(self, dD())
                            end
                            if tostring(self) == "Input" then
                                if bz[1] == "bv" then
                                    return wait(9e9)
                                end
                                if bz[1] == "hb" then
                                    return wait(9e9)
                                end
                                if bz[1] == "ws" then
                                    return wait(9e9)
                                end
                                if e and ab then
                                    if bz[2] and bz[2].mousehit then
                                        bz[2].mousehit = dD()
                                        return dT(self, unpack(bz))
                                    end
                                end
                            end
                            if
                                tostring(self.Parent) == "ReplicatedStorage" or
                                    bz[1] == "hey" and not tostring(self) == "SayMessageRequest"
                             then
                                return wait(9e9)
                            end
                            if tostring(self) == "Touch1" and h then
                                bz[3] = true
                                return dT(self, unpack(bz))
                            end
                            if bz[1] == "play" then
                                am = bz[2]
                            elseif bz[1] == "stop" then
                                am = nil
                            end
                        end
                        if dN == "WaitForChild" or dN == "FindFirstChild" then
                            if dK and dK ~= script and I and bz[1] == "HumanoidRootPart" then
                                bz[1] = "Torso"
                                return dT(self, unpack(bz))
                            end
                        end
                        return dT(self, ...)
                    end
                )
                if hookfunction then
                    local dU
                    dU =
                        hookfunction(
                        Instance.new "RemoteEvent".FireServer,
                        function(self, ...)
                            local bz = {...}
                            if tostring(self) == "Touch1" and h then
                                bz[3] = true
                                return dU(self, unpack(bz))
                            end
                            return dU(self, ...)
                        end
                    )
                end
                setreadonly(dI, true)
            else
                if game.PlaceId == 4669040 then
                    game:GetService("ReplicatedStorage").HelIo:Destroy()
                    wait()
                    game:GetService("ReplicatedStorage").lIIl:Destroy()
                    wait()
                    game:GetService("Players").LocalPlayer.Character:BreakJoints()
                end
            end
        end
    )
)
