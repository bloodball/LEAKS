    aI.Text = da
    aI.Visible = true
    wait(5)
    aI.Visible = false
end
local function dn(c7)
    if c7:IsA "Trail" then
        c7.Color = a9
    end
    if be[c7.Name] and T ~= "None" then
        wait()
        if c7.Name ~= "Shotty" and c7.Name ~= "Sawed Off" or T == "1" then
            GetChar().Humanoid:LoadAnimation(ay):Play()
        elseif c7.Name ~= "Glock" or T == "2" then
            GetChar().Humanoid:LoadAnimation(aA):Play()
        else
            local cT = GetChar().Humanoid:LoadAnimation(az)
            cT:Play()
            wait()
            cT:AdjustSpeed(0)
        end
    end
    if c7.Name == "Bone" then
        if i then
            GetChar().Humanoid.Health = 0
        end
        if y and game.PlaceId == 455366377 then
            for bA, cs in pairs(GetChar():GetDescendants()) do
                if cs:IsA "NumberValue" then
                    cs:Destroy()
                end
            end
        end
    end
    if c7:IsA "ObjectValue" and c7.Name == "creator" then
        local b6 = c7.Value
        if n then
            if e then
                ab = b6
                local cj
                cj =
                    Players:GetPlayerFromCharacter(ab).CharacterAdded:Connect(
                    function(ck)
                        if tostring(ck) == tostring(ab) then
                            ab = ck
                        else
                            cj:Disconnect()
                            cj = nil
                        end
                    end
                )
            end
            if r then
                af = Players:GetPlayerFromCharacter(b6)
            end
        end
        if m then
            CheckCommand("feloop " .. tostring(b6))
        end
        if l and not J then
            CheckCommand("triggerbot " .. tostring(b6))
            k = true
            local dp
            dp =
                Players:GetPlayerFromCharacter(b6).CharacterRemoving:Connect(
                function(dq)
                    if tostring(dq) == tostring(ad) then
                        J = false
                        g = false
                        ad = nil
                        b = false
                        v = false
                        i = false
                        k = false
                        ab = nil
                        dp:Disconnect()
                    else
                        dp:Disconnect()
                    end
                end
            )
        end
        pcall(
            function()
                local b_, dr = dl(b6)
                dm(
                    b6.Name ..
                        " has " ..
                            dr ..
                                " from " ..
                                    math.floor((GetChar().Head.Position - b6.Head.Position).magnitude) ..
                                        " studs with a " .. b_.Name
                )
            end
        )
    end
end
