                end
                if LP.Character then
                    local bu = bt:FindFirstChild "RealHumanoidRootPart" or bt:FindFirstChild "Torso"
                    if bu and p and bf["Shift"] then
                        if bf["W"] then
                            bu.CFrame = bu.CFrame * CFrame.new(0, 0, -V)
                        end
                        if bf["A"] then
                            bu.CFrame = bu.CFrame * CFrame.new(-V, 0, 0)
                        end
                        if bf["S"] then
                            bu.CFrame = bu.CFrame * CFrame.new(0, 0, V)
                        end
                        if bf["D"] then
                            bu.CFrame = bu.CFrame * CFrame.new(V, 0, 0)
                        end
                    end
                end
                for bA = 1, #aZ do
                    local cr = aZ[bA]
                    local b6 = cr["Player"]
                    if b6 and b6.Character then
                        local ef, eG =
                            b6.Character:FindFirstChild "Head",
                            b6.Character:FindFirstChild "RealHumanoidRootPart" or b6.Character:FindFirstChild "Torso"
                        if ef and eG and bt:FindFirstChild "Head" then
