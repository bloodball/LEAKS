                                                        "\nHas Glock: " ..
                                                            cn(b6, "Glock") ..
                                                                " | Shotty: " ..
                                                                    cn(b6, "Shotty") ..
                                                                        " | Vest: " ..
                                                                            cn(b6, "BulletResist") ..
                                                                                "\nLyAdmin User: " .. cA
                            cr["Text"].Position = Vector2.new(cP.X, cP.Y) + Vector2.new(0, 10)
                            cr["Box"][1].From = Vector2.new(eJ.X, eJ.Y)
                            cr["Box"][1].To = Vector2.new(eK.X, eK.Y)
                            cr["Box"][2].From = Vector2.new(eK.X, eK.Y)
                            cr["Box"][2].To = Vector2.new(eM.X, eM.Y)
                            cr["Box"][3].From = Vector2.new(eL.X, eL.Y)
                            cr["Box"][3].To = Vector2.new(eJ.X, eJ.Y)
                        end
                    end
                end
            end
        end
    )
