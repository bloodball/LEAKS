	if args[1] then
		clickdelKEY = string.sub(tostring(args[1]), 1, 1)
		clickdelACTIVE = true
		clickdelCLICK = false
	else
		clickdelKEY = ""
		clickdelACTIVE = true
		clickdelCLICK = true
	end
	clientSided()
end

