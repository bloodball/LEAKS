-- spinny boi
Commands.spin = function(args)
	local spinSpeed = 20
	if args[1] then
		spinSpeed = args[1]
	end
	for i,v in pairs(_player.c().HumanoidRootPart:GetChildren()) do
		if v.Name == "Spinning" then
			v:Destroy()
		end
	end
	local Spin = Instance.new("BodyAngularVelocity", _player.c().HumanoidRootPart)
	Spin.Name = "Spinning"
	Spin.MaxTorque = Vector3.new(0, math.huge, 0)
	Spin.AngularVelocity = Vector3.new(0,spinSpeed,0)
	noclip = not noclip
end

Commands.unspin = function(args)
_player.c().HumanoidRootPart.Spinning:Remove()
noclip = false
end

