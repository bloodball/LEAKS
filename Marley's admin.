function removeanti()
	local meta = debug.getmetatable(game)
	local newindex = meta.__newindex
	local namecall = meta.__namecall
	setreadonly(meta,false)
	
	meta.__newindex = newcclosure(function(self,key,value)
		if checkcaller() then 
			return newindex(self,key,value)
		end
		if self and self:IsA("Humanoid") then
			game.StarterGui:SetCore("ResetButtonCallback",true)
			if key == "WalkSpeed" then 
				return 16
			end
			if key == "JumpPower" then 
				return 0
			end
			if key == "HipHeight" then 
				return 0
			end
			if key == "Health" then 
				return
			end
		end
		if key == "CFrame" and self.Name == "Torso" or self.Name == "HumanoidRootPart" then 
			return
		end
		return newindex(self,key,value)
	end)
	meta.__namecall=function(self,...)
		if not checkcaller() then
			local method = getnamecallmethod()
			if method == "ClearAllChildren" or method == "Destroy" then 
				return 
			end 
		end
		return namecall(self,...)
	end
	
	
	meta.__namecall = newcclosure(function(self,...)
		local args = {...}
		local t = getnamecallmethod()
		if t == "BreakJoints" then 
			return 0
		end
		if t == "Kick" then
			return wait()
		end
		if t == "FireServer" then 
			if self.Name == "lIII" or self.Parent.Name == "ReplicatedStorage" then 
				return wait()
			end
			if args[1] == "hey" then 
				return wait()
			end
		end
		return namecall(self,...)
	end)
	end

Commands.bypass = function(args)
	if args[1] then
		if string.lower(tostring(args[1])) == "on" then
			antikick()
			Notification("warning", "(ENABLED) Most Anti-Cheats wont detect this, Unless it uses the same method.", 7)
		elseif string.lower(tostring(args[1])) == "off" then
			removeanti()
			Notification("warning","(DISABLED) You will get kicked for using anything that give you advantages i.e Walkspeed etc")
		end
	end
end

