	a.Parent = gsWorkspace
	a.Size = Vector3.new(10, 1, 10)
	a.Anchored = true
	a.CFrame = LP.Character.HumanoidRootPart.CFrame + Vector3.new(0, 5, 0)
	LP.Character.HumanoidRootPart.CFrame = a.CFrame + Vector3.new(0, 2, 0)
	clientSided()
	wait(20)
	a:Destroy()
end

