Commands.bplay = function(args)
		local ScreenGui = Instance.new("ScreenGui")
		local invframe = Instance.new("Frame")
		local Play = Instance.new("TextButton")
		local ID = Instance.new("TextBox")
		local TP = Instance.new("TextBox")
		local next1 = Instance.new("TextButton")
		
		ScreenGui.Name = ""
		ScreenGui.Parent = game.CoreGui
		ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
		ScreenGui.ResetOnSpawn = true
		
		invframe.Name = "invframe"
		invframe.Parent = ScreenGui
		invframe.BackgroundColor3 = Color3.new(1, 1, 1)
		invframe.BackgroundTransparency = 1
		invframe.Position = UDim2.new(0.343317986, -50, 0.96963948, -100)
		invframe.Size = UDim2.new(0, 455, 0, 116)
		
		Play.Name = "Play"
		Play.Parent = invframe
		Play.BackgroundColor3 = Color3.new(0, 0, 0)
		Play.BackgroundTransparency = 0.5
		Play.BorderColor3 = Color3.new(0, 0, 0)
		Play.BorderSizePixel = 0
		Play.Position = UDim2.new(0.787912071, -18, 0.18965508, -21)
		Play.Size = UDim2.new(0, 36, 0, 21)
		Play.Visible = false
		Play.Font = Enum.Font.SourceSans
		Play.Text = "Play"
		Play.TextColor3 = Color3.new(1, 1, 1)
		Play.TextSize = 15
		Play.MouseButton1Down:connect(function()
		local name = game.Players.LocalPlayer.Name
		local id = ID.Text
		local zeros = 99436
		
			   if TP.Visible == true then
				   wait(.5)
				Play.Visible = false
				next1.Visible = true
				ID.Visible = true
				ID.Text = "ID HERE"
				TP.Visible = false
			   end
		
		for _, tool in ipairs(game.Players[name].Backpack:GetChildren()) do
			 if tool.name == "BoomBox" or tool.name == "Boombox" or tool.name == "SuperFlyGoldBoombox" then
				  tool.Parent = game.Players[name].Character
			 end
		end
		
		for _,v in pairs(game.Workspace[name]:GetChildren()) do 
			if v.Name == "BoomBox" or v.Name == "Boombox" or v.Name == "SuperFlyGoldBoombox" then 
				id = ("0"):rep(zeros) .. id
				v.Remote:FireServer("PlaySong", ""..id)
			end
		end
		
		wait(0.6)
		
		for _, tool in ipairs(game:GetService("Workspace")[name]:GetChildren()) do
			 if tool.name == "BoomBox" or tool.name == "Boombox" or tool.name == "SuperFlyGoldBoombox" then
				  game.Players[name].Character.Humanoid:UnequipTools()
			 end
		end
		
		wait(0.6)
		
		wait(0.6)
		for _,v in pairs(game.Players[name].Backpack:GetChildren()) do 
			if v.Name == "BoomBox" or v.Name == "Boombox" or v.Name == "SuperFlyGoldBoombox" then 
				v.Handle.Sound.TimePosition = TP.Text
				v.Handle.Sound.Playing = true
			end
			end
		end)
		
		ID.Name = "ID"
		ID.Parent = invframe
		ID.BackgroundColor3 = Color3.new(0.207843, 0.207843, 0.207843)
		ID.BackgroundTransparency = 0.5
		ID.BorderSizePixel = 0
		ID.Position = UDim2.new(0.362252742, 0, 0.00931034982, 0)
		ID.Size = UDim2.new(0, 176, 0, 21)
		ID.Font = Enum.Font.SourceSans
		ID.PlaceholderColor3 = Color3.new(1, 1, 1)
		ID.PlaceholderText = "ID HERE"
		ID.Text = ""
		ID.TextColor3 = Color3.new(1, 1, 1)
		ID.TextSize = 17
		ID.TextWrapped = true
		
		TP.Name = "TP"
		TP.Parent = invframe
		TP.BackgroundColor3 = Color3.new(0.207843, 0.207843, 0.207843)
		TP.BackgroundTransparency = 0.5
		TP.BorderSizePixel = 0
		TP.Position = UDim2.new(0.364450574, 0, 0.00862068962, 0)
		TP.Size = UDim2.new(0, 175, 0, 21)
		TP.Visible = false
		TP.Font = Enum.Font.SourceSans
		TP.PlaceholderColor3 = Color3.new(1, 1, 1)
		TP.PlaceholderText = "TimePosition"
		TP.Text = ""
		TP.TextColor3 = Color3.new(1, 1, 1)
		TP.TextSize = 17
		TP.TextWrapped = true
		
		next1.Name = "next"
		next1.Parent = invframe
		next1.BackgroundColor3 = Color3.new(0, 0, 0)
		next1.BackgroundTransparency = 0.5
		next1.BorderColor3 = Color3.new(0, 0, 0)
		next1.BorderSizePixel = 0
		next1.Position = UDim2.new(0.787912071, -18, 0.18965508, -21)
		next1.Size = UDim2.new(0, 36, 0, 21)
		next1.Font = Enum.Font.SourceSans
		next1.Text = "next"
		next1.TextColor3 = Color3.new(1, 1, 1)
		next1.TextSize = 15
		next1.MouseButton1Down:connect(function()
			if ID.Visible == true then
				ID.Visible = false
				next1.Visible = false
				Play.Visible = true
				TP.Visible = true
				TP.Text = "TimePosition"
		end
	end)
end

stutterON = false
Commands.stutter = function(args)
	if args[1] then
		if string.lower(tostring(args[1])) == "on" then
			stutterON = true
		elseif string.lower(tostring(args[1])) == "off" then
			stutterON = false
			wait(0.4)
			LP.Character.HumanoidRootPart.Anchored = false
		end 
	end
end

spawn(function()
	while wait(0.1) do
		if stutterON == true then
			LP.Character.HumanoidRootPart.Anchored = false
			wait(0.1)
			LP.Character.HumanoidRootPart.Anchored = true
		end
	end
end)

